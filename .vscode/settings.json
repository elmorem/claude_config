    "**/todos/**": true,
    "**/statsig/**": true,
    "**/shell-snapshots/**": true,
    "**/ide/**": true
  },
  "search.exclude": {
    "**/projects/**": true,
    "**/todos/**": true,
    "**/statsig/**": true,
    "**/shell-snapshots/**": true,
    "**/ide/**": true
  }
}